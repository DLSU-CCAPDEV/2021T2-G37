<div class="wish" id={{pNum}}>
    <img class="image" src={{pImage}}>
    <div class="details">
        <h2>{{pName}}</h2>
        <h3>Variation: {{pColor}}</h3>
        <h3>Size: {{pSize}}</h3>
    </div>
    <div class="price"><h3>P {{pPrice}}</h3></div>
    <div class="buttons">
        <button class="add-cart"></button>
        <button class="delete"></button>
    </div>
</div>